title "Bathymetry"

addTimeSeries "Altitude": "EstimatedState.alt", "Depth":"EstimatedState.depth"

depth = value "Depth"

alt = value "Altitude"

sum = { double1, double2 -> double1+double2}

bathym = apply ("Bathymetry", depth, alt,sum)

deepestP = getTimeSeriesMaxItem bathym

markFromItem "Deepest Point", deepestP

addTimeSeries bathym

