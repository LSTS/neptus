<?xml version="1.0" encoding="UTF-8"?>
<config format="1" version="2016.01.0 (master,dd196ec,dirty)" system="doam" i18n="en_US">
  <section name="Thermal Zone" name-i18n="Thermal Zone">
    <param name="Entity Label">
      <name-i18n>Entity Label</name-i18n>
      <type>string</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>Thermal Zone</default>
      <units/>
      <desc>Main entity label</desc>
    </param>
    <param name="Execution Priority">
      <name-i18n>Execution Priority</name-i18n>
      <type>integer</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>10</default>
      <units/>
      <desc>Execution priority</desc>
    </param>
    <param name="Activation Time">
      <name-i18n>Activation Time</name-i18n>
      <type>integer</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>0</default>
      <units/>
      <desc/>
    </param>
    <param name="Deactivation Time">
      <name-i18n>Deactivation Time</name-i18n>
      <type>integer</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>0</default>
      <units/>
      <desc/>
    </param>
    <param name="Debug Level">
      <name-i18n>Debug Level</name-i18n>
      <type>string</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>None</default>
      <units/>
      <desc/>
      <values>None, Debug, Trace, Spew</values>
      <values-i18n>None, Debug, Trace, Spew</values-i18n>
    </param>
    <param name="Execution Frequency">
      <name-i18n>Execution Frequency</name-i18n>
      <type>real</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>1.0</default>
      <units>Hz</units>
      <desc>Frequency at which task is executed</desc>
    </param>
    <param name="Path">
      <name-i18n>Path</name-i18n>
      <type>string</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>/sys/class/thermal/thermal_zone0/temp</default>
      <units/>
      <desc>Path to the sysfs file</desc>
    </param>
    <param name="Entity Label - Temperature">
      <name-i18n>Entity Label - Temperature</name-i18n>
      <type>string</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>Mainboard (Core)</default>
      <units/>
      <desc>Entity label of temperature sensor</desc>
    </param>
  </section>
