  <section name="Camera" name-i18n="Camera">
    <param name="Entity Label">
      <name-i18n>Entity Label</name-i18n>
      <type>string</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>Camera</default>
      <units/>
      <desc>Main entity label</desc>
    </param>
    <param name="Execution Priority">
      <name-i18n>Execution Priority</name-i18n>
      <type>integer</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>10</default>
      <units/>
      <desc>Execution priority</desc>
    </param>
    <param name="Activation Time">
      <name-i18n>Activation Time</name-i18n>
      <type>integer</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>0</default>
      <units/>
      <desc/>
    </param>
    <param name="Deactivation Time">
      <name-i18n>Deactivation Time</name-i18n>
      <type>integer</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>0</default>
      <units/>
      <desc/>
    </param>
    <param name="Debug Level">
      <name-i18n>Debug Level</name-i18n>
      <type>string</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>None</default>
      <units/>
      <desc/>
      <values>None, Debug, Trace, Spew</values>
      <values-i18n>None, Debug, Trace, Spew</values-i18n>
    </param>
    <param name="Active - Scope">
      <name-i18n>Active - Scope</name-i18n>
      <type>string</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>maneuver</default>
      <units/>
      <desc>Scoped of the 'Active' parameter</desc>
    </param>
    <param name="Active - Visibility">
      <name-i18n>Active - Visibility</name-i18n>
      <type>string</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>user</default>
      <units/>
      <desc>Visibility of the 'Active' parameter</desc>
    </param>
    <param name="Active">
      <name-i18n>Active</name-i18n>
      <type>boolean</type>
      <visibility>user</visibility>
      <scope>maneuver</scope>
      <default>0</default>
      <units/>
      <desc>True to activate task, false otherwise</desc>
    </param>
    <param name="Camera IPv4 Address">
      <name-i18n>Camera IPv4 Address</name-i18n>
      <type>ipv4-address</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>10.0.10.52</default>
      <units/>
      <desc>IPv4 address of the camera</desc>
    </param>
    <param name="Local IPv4 Address">
      <name-i18n>Local IPv4 Address</name-i18n>
      <type>ipv4-address</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>10.0.10.53</default>
      <units/>
      <desc>Local IPv4 address</desc>
    </param>
    <param name="Frames Per Second">
      <name-i18n>Frames Per Second</name-i18n>
      <type>integer</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>3</default>
      <units/>
      <desc>Frames per second</desc>
    </param>
    <param name="Stream Port">
      <name-i18n>Stream Port</name-i18n>
      <type>integer</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>61200</default>
      <units/>
      <desc>Local UDP port used to receive the video stream</desc>
    </param>
    <param name="Exposure Time">
      <name-i18n>Exposure Time</name-i18n>
      <type>real</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>0.00800</default>
      <units>s</units>
      <desc>Exposure time</desc>
      <min>0.0001</min>
      <max>30.0</max>
    </param>
    <param name="Automatic Exposure">
      <name-i18n>Automatic Exposure</name-i18n>
      <type>boolean</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>true</default>
      <units/>
      <desc>Automatic Exposure (maximum exposure time set to Exposure Time)</desc>
    </param>
    <param name="Automatic Exposure - Minimum Value">
      <name-i18n>Automatic Exposure - Minimum Value</name-i18n>
      <type>real</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>0.002</default>
      <units/>
      <desc>Minimum exposure value when using automatic exposure filter</desc>
    </param>
    <param name="Gain">
      <name-i18n>Gain</name-i18n>
      <type>integer</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>260</default>
      <units/>
      <desc>Gain</desc>
      <min>260</min>
      <max>1023</max>
    </param>
    <param name="Gamma">
      <name-i18n>Gamma</name-i18n>
      <type>integer</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>120</default>
      <units/>
      <desc>Gamma</desc>
      <min>1</min>
      <max>500</max>
    </param>
    <param name="Strobe Mode">
      <name-i18n>Strobe Mode</name-i18n>
      <type>string</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>Exposure</default>
      <units/>
      <desc>Strobe mode</desc>
      <values>Off, Always On, Fixed, Exposure</values>
      <values-i18n>Off, Always On, Fixed, Exposure</values-i18n>
    </param>
    <param name="Strobe Polarity">
      <name-i18n>Strobe Polarity</name-i18n>
      <type>boolean</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>false</default>
      <units/>
      <desc>Strobe polarity</desc>
    </param>
    <param name="Strobe Delay">
      <name-i18n>Strobe Delay</name-i18n>
      <type>integer</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>0</default>
      <units>us</units>
      <desc>Strobe delay</desc>
      <min>-10000</min>
      <max>10000</max>
    </param>
    <param name="Strobe Duration">
      <name-i18n>Strobe Duration</name-i18n>
      <type>integer</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>0</default>
      <units/>
      <desc>Strobe duration</desc>
      <min>0</min>
      <max>20000</max>
    </param>
    <param name="Number of Buffers">
      <name-i18n>Number of Buffers</name-i18n>
      <type>integer</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>100</default>
      <units/>
      <desc>Number of buffers</desc>
    </param>
    <param name="JPEG Quality">
      <name-i18n>JPEG Quality</name-i18n>
      <type>integer</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>80</default>
      <units>%</units>
      <desc>JPEG image quality</desc>
      <min>0</min>
      <max>100</max>
    </param>
    <param name="Store Raw">
      <name-i18n>Store Raw</name-i18n>
      <type>boolean</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>false</default>
      <units/>
      <desc>Store raw image data in PGM format</desc>
    </param>
    <param name="White Balance - B Factor">
      <name-i18n>White Balance - B Factor</name-i18n>
      <type>real</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>1.0</default>
      <units/>
      <desc/>
    </param>
    <param name="White Balance - R Factor">
      <name-i18n>White Balance - R Factor</name-i18n>
      <type>real</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>1.0</default>
      <units/>
      <desc/>
    </param>
  </section>
</config>
