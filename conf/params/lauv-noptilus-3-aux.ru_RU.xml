  <section name="Power Supervisor" name-i18n="Power Supervisor">
    <param name="Entity Label">
      <name-i18n>Entity Label</name-i18n>
      <type>string</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>Power Supervisor</default>
      <units/>
      <desc>Main entity label</desc>
    </param>
    <param name="Execution Priority">
      <name-i18n>Execution Priority</name-i18n>
      <type>integer</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>10</default>
      <units/>
      <desc>Execution priority</desc>
    </param>
    <param name="Activation Time">
      <name-i18n>Activation Time</name-i18n>
      <type>integer</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>0</default>
      <units/>
      <desc/>
    </param>
    <param name="Deactivation Time">
      <name-i18n>Deactivation Time</name-i18n>
      <type>integer</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>0</default>
      <units/>
      <desc/>
    </param>
    <param name="Debug Level">
      <name-i18n>Debug Level</name-i18n>
      <type>string</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>None</default>
      <units/>
      <desc/>
      <values>None, Debug, Trace, Spew</values>
      <values-i18n>None, Debug, Trace, Spew</values-i18n>
    </param>
    <param name="Main Power Channel">
      <name-i18n>Main Power Channel</name-i18n>
      <type>string</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>System</default>
      <units/>
      <desc>Name of the main power channel</desc>
    </param>
    <param name="Command - On Power Down">
      <name-i18n>Command - On Power Down</name-i18n>
      <type>string</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>services syslog stop &amp;&amp; mount -o remount,ro /opt &amp;&amp; mount -o remount,ro /opt/lsts/dune/log</default>
      <units/>
      <desc>Command to execute before powering down the system</desc>
    </param>
    <param name="Command - On Power Down Abort">
      <name-i18n>Command - On Power Down Abort</name-i18n>
      <type>string</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>mount -o remount,rw /opt/lsts/dune/log &amp;&amp; mount -o remount,rw /opt &amp;&amp; services syslog restart</default>
      <units/>
      <desc>Command to execute when the power down sequence is aborted</desc>
    </param>
    <param name="Slave System Names">
      <name-i18n>Slave System Names</name-i18n>
      <type>list:string</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default/>
      <units/>
      <desc>Name of the slave systems</desc>
    </param>
  </section>
