<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:annotation>
		<xs:documentation>v0.9</xs:documentation>
	</xs:annotation>
	<xs:include schemaLocation="neptus-types.xsd"/>
	<xs:complexType name="pointType">
		<xs:annotation>
			<xs:documentation>3D point</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="point" type="locationType"/>
			<xs:element name="radiusTolerance">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:float"/>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="attitude" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="roll">
							<xs:annotation>
								<xs:documentation>in degrees (º)</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:float">
										<xs:attribute name="rollTolerance" type="xs:float" use="required"/>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="pitch">
							<xs:annotation>
								<xs:documentation>in degrees (º)</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:float">
										<xs:attribute name="pitchTolerance" type="xs:float" use="required"/>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="yaw">
							<xs:annotation>
								<xs:documentation>in degrees (º)</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:float">
										<xs:attribute name="yawTolerance" type="xs:float" use="required"/>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="coordinate-system" minOccurs="0">
							<xs:simpleType>
								<xs:restriction base="xs:token"/>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ManeuverType">
		<xs:sequence>
			<xs:sequence>
				<xs:element name="minTime" type="xs:float" default="0" minOccurs="0"/>
				<xs:element name="maxTime" type="xs:float" minOccurs="0"/>
			</xs:sequence>
			<xs:choice>
				<xs:element ref="Goto"/>
				<xs:element ref="Loiter"/>
				<xs:element ref="StationKeeping"/>
				<xs:element ref="Unconstrained"/>
				<xs:element ref="Dive"/>
				<xs:element ref="PopUp"/>
				<xs:element ref="Elevator"/>
				<xs:element ref="YoYo"/>
				<xs:element ref="FollowSystem"/>
				<xs:element ref="FollowPath"/>
				<xs:element ref="FollowTrajectory"/>
				<xs:element ref="VehicleFormation"/>
				<xs:element ref="Rows"/>
				<xs:element ref="RowsManeuver"/>
				<xs:element ref="RowsPattern"/>
				<xs:element ref="RIPattern"/>
				<xs:element ref="CrossHatchPattern"/>
				<xs:element ref="HeadingSpeedDepth"/>
				<xs:element ref="SubPlan"/>
				<xs:element name="IMCManeuver">
					<xs:complexType>
						<xs:sequence>
							<xs:sequence>
								<xs:any namespace="##any"/>
							</xs:sequence>
							<xs:element name="annotation" type="annotationType" minOccurs="0"/>
						</xs:sequence>
						<xs:attribute name="name" use="required"/>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:sequence minOccurs="0">
				<xs:element name="custom-settings" minOccurs="0">
					<xs:complexType>
						<xs:sequence maxOccurs="unbounded">
							<xs:element name="setting">
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:string">
											<xs:attribute name="name" type="xs:string" use="required"/>
											<xs:attribute name="type-hint" default="String">
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:enumeration value="String"/>
														<xs:enumeration value="Number"/>
														<xs:enumeration value="Boolean"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="FollowPath">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="basePoint">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="pointType">
								<xs:attribute name="type" type="xs:string" use="required" fixed="pointType"/>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="path">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="nedOffsets" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:string">
											<xs:attribute name="northOffset" type="xs:double" use="required"/>
											<xs:attribute name="eastOffset" type="xs:double" use="required"/>
											<xs:attribute name="depthOffset" type="xs:double" use="required"/>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="speed" type="speedType" minOccurs="0"/>
				<xs:element name="annotation" type="annotationType" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="kind" type="xs:string" use="required" fixed="automatic"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Goto">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="initialPoint" minOccurs="0">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="pointType">
								<xs:attribute name="type" type="xs:string" use="required" fixed="pointType"/>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="finalPoint">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="pointType">
								<xs:attribute name="type" type="xs:string" use="required" fixed="pointType"/>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:choice>
					<xs:element name="speed" type="speedType"/>
					<xs:element name="velocity" type="speedType">
						<xs:annotation>
							<xs:documentation>@Deprecated - use speed instead</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
				<xs:element name="trajectoryTolerance">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="radiusTolerance">
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:float">
											<xs:attribute name="type" type="xs:string" fixed="float"/>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="annotation" type="annotationType" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="kind" type="xs:string" use="required" fixed="automatic"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Loiter">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="basePoint" minOccurs="0">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="pointType">
								<xs:attribute name="type" type="xs:string" use="required" fixed="pointType"/>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="duration" type="xs:int">
					<xs:annotation>
						<xs:documentation>The duration of the Loiter maneuver, in seconds.
-1 = Infinity
 0 = No Loiter</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="trajectory" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="radius">
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:float">
											<xs:attribute name="type" type="xs:string" use="required" fixed="float"/>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="radiusTolerance">
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:float">
											<xs:attribute name="type" type="xs:string" use="required" fixed="float"/>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="type" default="Hover" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="Circular"/>
										<xs:enumeration value="Racetrack"/>
										<xs:enumeration value="Figure 8"/>
										<xs:enumeration value="Hover"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="length" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Basicaly for racetrack, and figure8.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:float">
											<xs:attribute name="type" type="xs:string" use="required" fixed="float"/>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="bearing" minOccurs="0">
								<xs:annotation>
									<xs:documentation>in degrees</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:float">
											<xs:attribute name="type" type="xs:string" use="required" fixed="float"/>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="direction" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="Vehicle Dependent"/>
										<xs:enumeration value="Clockwise"/>
										<xs:enumeration value="Counter-Clockwise"/>
										<xs:enumeration value="Into the wind"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:choice minOccurs="0">
					<xs:element name="speed" type="speedType" minOccurs="0"/>
					<xs:element name="velocity" type="speedType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>@Deprecated - use speed instead</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
				<xs:element name="annotation" type="annotationType" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="kind" type="xs:string" use="required" fixed="automatic"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="StationKeeping">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="basePoint" minOccurs="0">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="pointType">
								<xs:attribute name="type" type="xs:string" use="required" fixed="pointType"/>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="duration" type="xs:int">
					<xs:annotation>
						<xs:documentation>The duration of the Loiter maneuver, in seconds.
-1 = Infinity
 0 = No Loiter</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="trajectory" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="radius">
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:float">
											<xs:attribute name="type" type="xs:string" use="required" fixed="float"/>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="speed" type="speedType" minOccurs="0"/>
				<xs:element name="annotation" type="annotationType" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="kind" type="xs:string" use="required" fixed="automatic"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Unconstrained">
		<xs:complexType>
			<xs:sequence minOccurs="0">
				<xs:element name="annotation" type="annotationType" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="kind" type="xs:string" use="required" fixed="manual"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Dive">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="depth" type="xs:float">
					<xs:annotation>
						<xs:documentation>in metres</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:choice>
					<xs:element name="speed" type="speedType"/>
					<xs:element name="velocity" type="speedType">
						<xs:annotation>
							<xs:documentation>@Deprecated - use speed instead</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
				<xs:element name="heading">
					<xs:annotation>
						<xs:documentation>In degrees. The heading to be used on the dive. Use '-1' for this field to be disconsidered. Otherwise the value spans from 0 to 2Pi.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:double">
							<xs:minInclusive value="-1"/>
							<xs:maxInclusive value="360"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="annotation" type="annotationType" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="kind" type="xs:string" use="required" fixed="automatic"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="PopUp">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="finalPoint">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="pointType">
								<xs:attribute name="type" type="xs:string" use="required" fixed="pointType"/>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="duration">
					<xs:annotation>
						<xs:documentation>in seconds</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="xs:int"/>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
				<xs:choice>
					<xs:element name="speed" type="speedType"/>
					<xs:element name="velocity" type="speedType">
						<xs:annotation>
							<xs:documentation>@Deprecated - use speed instead</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
				<xs:element name="annotation" type="annotationType" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="kind" type="xs:string" use="required" fixed="automatic"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="HeadingSpeedDepth">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="duration">
					<xs:annotation>
						<xs:documentation>in seconds</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="xs:int"/>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="depth" type="xs:float">
					<xs:annotation>
						<xs:documentation>in metres</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:choice>
					<xs:element name="speed" type="speedType"/>
					<xs:element name="velocity" type="speedType">
						<xs:annotation>
							<xs:documentation>@Deprecated - use speed instead</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
				<xs:element name="heading">
					<xs:annotation>
						<xs:documentation>In degrees. The heading to be used on the dive. Use '-1' for this field to be disconsidered. Otherwise the value spans from 0 to 2Pi.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:double">
							<xs:minInclusive value="-1"/>
							<xs:maxInclusive value="360"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="annotation" type="annotationType" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="kind" type="xs:string" use="required" fixed="automatic"/>
			<xs:attribute name="useHeading" type="xs:boolean" default="true"/>
			<xs:attribute name="useSpeed" type="xs:boolean" default="true"/>
			<xs:attribute name="useDepth" type="xs:boolean" default="true"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="HeadingVelocityDepth">
		<xs:annotation>
			<xs:documentation>@Deprecated</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="duration">
					<xs:annotation>
						<xs:documentation>in seconds</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="xs:int"/>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="depth" type="xs:float">
					<xs:annotation>
						<xs:documentation>in metres</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:choice>
					<xs:element name="speed" type="speedType"/>
					<xs:element name="velocity" type="speedType">
						<xs:annotation>
							<xs:documentation>@Deprecated - use speed instead</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
				<xs:element name="heading">
					<xs:annotation>
						<xs:documentation>In degrees. The heading to be used on the dive. Use '-1' for this field to be disconsidered. Otherwise the value spans from 0 to 2Pi.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:double">
							<xs:minInclusive value="-1"/>
							<xs:maxInclusive value="360"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="annotation" type="annotationType" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="kind" type="xs:string" use="required" fixed="automatic"/>
			<xs:attribute name="useHeading" type="xs:boolean" default="true"/>
			<xs:attribute name="useVelocity" type="xs:boolean" default="true"/>
			<xs:attribute name="useDepth" type="xs:boolean" default="true"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="YoYo">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="initialPoint" minOccurs="0">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="pointType">
								<xs:attribute name="type" type="xs:string" use="required" fixed="pointType"/>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="finalPoint">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="pointType">
								<xs:attribute name="type" type="xs:string" use="required" fixed="pointType"/>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="amplitude" type="xs:float"/>
				<xs:element name="pitch">
					<xs:simpleType>
						<xs:restriction base="xs:float">
							<xs:minInclusive value="0"/>
							<xs:maxInclusive value="0.7853982"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="speed" type="speedType"/>
				<xs:element name="annotation" type="annotationType" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="kind" type="xs:string" use="required" fixed="automatic"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="FollowTrajectory">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="basePoint">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="pointType">
								<xs:attribute name="type" type="xs:string" use="required" fixed="pointType"/>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="trajectory">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="nedOffsets" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:string">
											<xs:attribute name="northOffset" type="xs:double" use="required"/>
											<xs:attribute name="eastOffset" type="xs:double" use="required"/>
											<xs:attribute name="depthOffset" type="xs:double" use="required"/>
											<xs:attribute name="timeOffset" type="xs:double" use="required"/>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="speed" type="speedType" minOccurs="0"/>
				<xs:element name="annotation" type="annotationType" minOccurs="0"/>
			</xs:sequence>
